#include <iostream> 
using namespace std; 
  
class Compare { 
public: 
    int comp(Compare E) 
    { 
        if(a >= E.a)
    } 
}; 
  
// Driver Code 
int main() 
{ 
    int small;
    Compare E1, E2; 
  
    E1.a = 50; 
    E2.a = 100; 
  
